{
  "name": "stylelint-config-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "immutable": "^3.8.2",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-render-html": "^0.6.0",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "styled-components": "^3.4.5",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:js": "eslint --ext .jsx --ext .js src/",
    "lint:sc": "stylelint ./src/**/*.jsx",
    "lint:css": "stylelint ./src/**/*.css --config ./stylelintCSSConfig.json",
    "lint": "yarn lint:js && yarn lint:sc && yarn lint:css",
    "prepush": "yarn lint"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "husky": "^0.14.3",
    "now": "^11.4.1",
    "stylelint": "^9.5.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2"
  },
  "now": {
    "alias": "stylelint-config-generator"
  }
}
